@article{couture_merging_2017,
	title = {Merging arts and bioethics: {An} interdisciplinary experiment in cultural and scientific mediation},
	volume = {31},
	issn = {1467-8519},
	shorttitle = {Merging arts and bioethics},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/bioe.12391/abstract},
	doi = {10.1111/bioe.12391},
	abstract = {How to engage the public in a reflection on the most pressing ethical issues of our time? What if part of the solution lies in adopting an interdisciplinary and collaborative strategy to shed light on critical issues in bioethics? An example is Art + Bioéthique, an innovative project that brought together bioethicists, art historians and artists with the aim of expressing bioethics through arts in order to convey the “sensitive” aspect of many health ethics issues. The aim of this project was threefold: 1) to identify and characterize mechanisms for the meeting of arts and bioethics; 2) to experiment with and co-construct a dialogue between arts and bioethics; and 3) to initiate a public discussion on bioethical issues through the blending of arts and bioethics. In connection with an exhibition held in March 2016 at the Espace Projet, a non-profit art space in Montréal (Canada), the project developed a platform that combined artworks, essays and cultural \& scientific mediation activities related to the work of six duos of young bioethics researchers and emerging artists. Each duo worked on a variety of issues, such as the social inclusion of disabled people, the challenges of practical applications of nanomedicine and regenerative medicine, and a holistic approach to contemporary diseases. This project, which succeeded in stimulating an interdisciplinary dialogue and collaboration between bioethics and arts, is an example of an innovative approach to knowledge transfer that can move bioethics reflection into the public space.},
	language = {en},
	number = {8},
	journal = {Bioethics},
	author = {Couture, Vincent and Bélisle-Pipon, Jean-Christophe and Cloutier, Marianne and Barnabé, Catherine},
	month = oct,
	year = {2017},
	keywords = {arts, bioethics, interdisciplinary research, Knowledge transfer},
	pages = {616--630},
	file = {Full Text PDF:C\:\\Users\\Jean-Christophe\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\tvrwyuz2.default\\zotero\\storage\\7H3B2GGE\\Couture et al. - 2017 - Merging arts and bioethics An interdisciplinary e.pdf:application/pdf;Snapshot:C\:\\Users\\Jean-Christophe\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\tvrwyuz2.default\\zotero\\storage\\7MB3HQKU\\abstract.html:text/html}
